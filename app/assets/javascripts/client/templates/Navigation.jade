      MenuIcon(act="SIGN_OUT" src="images/kaffcop_icon/fab_out.png")
    else
      MenuIcon(act="MOVE_TOP" src="images/kaffcop_icon/fab_home.png")
      MenuIcon(act="SIGN_IN" src="images/kaffcop_icon/fab_in.png") 
